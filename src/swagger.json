{
    "openapi": "3.0.0",
    "info": {
        "title": "AutoShift Documentation",
        "description": "API for car rentals",
        "version": "1.0.0",
        "contact": {
            "email": "giovannemika18@gmail.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": [
                    "Category"
                ],
                "summary": "Create a category",
                "description": "Create a new category of cars",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                }
                            },
                            "example": {
                                "name": "CategoryTest",
                                "description": "DescriptionTest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Category already exists"
                    }
                }
            },
            "get": {
                "tags": [
                    "Category"
                ],
                "summary": "Search for all categories",
                "description": "Search for all categories of the cars",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content":{
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}